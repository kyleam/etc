alias to='pushd'
alias ot='popd'
alias ..='cd ..'
alias cd..='cd ..'
alias cd...='cd ../..'
alias cd....='cd ../../..'
alias cd.....='cd ../../../..'
alias md='mkdir -p'
alias rd=rmdir
alias d='dirs -v | head -10'
alias td='tree -d'
alias ls='ls --color'
alias la='ls -Ah'
alias lla='ls -lAhL'
alias l='ls -lhL'
