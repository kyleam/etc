*.bib    diff=bibtex
*.c      diff=cpp
*.h      diff=cpp
*.cpp    diff=cpp
*.hpp    diff=cpp
*.cc     diff=cpp
*.hh     diff=cpp
*.py     diff=python
*.pl     diff=perl
*.html   diff=html
*.xhtml  diff=html

*.lisp   diff=lisp
*.el     diff=lisp
*.r      diff=rstats
